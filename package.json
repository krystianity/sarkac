{
  "name": "sarkac",
  "version": "0.10.0",
  "description": "Apache Kafka message payloads anomaly detection",
  "scripts": {
    "start": "TZ='Europe/Berlin' NODE_ENV=production DEBUG=sarkac:* node example/example.js",
    "produce": "TZ='Europe/Berlin' NODE_ENV=production DEBUG=sarkac:* node example/produceExampleMessages.js",
    "fix": "eslint --fix .",
    "lint": "eslint .",
    "test": "yarn lint && yarn test:only",
    "test:only": "TZ='Europe/Berlin' NODE_ENV=test _mocha -R spec --exit --timeout 5000 './test/int/**/*.js'",
    "qcommit": "git add . && git commit -m 'auto-generated-commit' && git push origin master"
  },
  "dependencies": {
    "async": "~2.6.1",
    "bluebird": "~3.5.1",
    "body-parser": "~1.18.3",
    "cors": "v2.8.4",
    "debug": "~3.1.0",
    "express": "~4.16.3",
    "juration": "~0.1.0",
    "kafka-streams": "~4.5.0",
    "mongoose": "~5.1.7",
    "murmurhash": "~0.0.2",
    "sinek": "~6.18.0"
  },
  "devDependencies": {
    "eslint": "~5.0.1",
    "mocha": "~5.2.0",
    "request": "~2.87.0"
  }
}
